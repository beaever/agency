{
  "$schema": "https://json.schemastore.org/swcrc",

  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "dynamicImport": true,
      "decorators": true
    },
    "transform": {
      "decoratorMetadata": true,
      "legacyDecorator": true
    },
    "target": "es2016",
    "loose": false,
    "externalHelpers": false,
    "keepClassNames": false
  },
  "module": {
    "type": "commonjs",
    "strict": false,
    "strictMode": true,
    "noInterop": false,
    "lazy": false
  },
  "sourceMaps": true,
  "minify": true,
  "isModule": true
}
